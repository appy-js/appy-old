{
  "nixEnvSelector.nixFile": "${workspaceRoot}/shell.nix",
  "deno.enable": true,
  "deno.importMap": "import_map.json",
  "deno.lint": true,
  "deno.unstable": true,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "editor.formatOnSave": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true
  }
}
